		"Constraints": {
			"Constraint": [
				{
					"Condition": " (  Inserting  OR  Updating )   AND  (  ITM_TYP   <> 5 /* Folder */ AND APPL_ID = 0 /* Common Application */ ) ",
					"ErrorMessage": "Only folders can belong to the Common Application",
					"_Name": "Common Folder Check",
					"_ConditionType": "RejectWhen",
					"_ErrorAttribute": "APPL_ID"
				},
				{
					"Condition": " Updating  AND  isChanged( 'APPL_ID' ) = true ",
					"ErrorMessage": "The Application cannot be changed on an existing entry",
					"_Name": "Application Change",
					"_ConditionType": "RejectWhen",
					"_ErrorAttribute": "APPL_ID"
				},
				{
					"Condition": " ( Inserting  OR  Updating )  AND (   isNull( 'PNT_ID' ) = false  )  AND  ( checkParentChainJobApplication() = false ) ",
					"ErrorMessage": "The Application must match with the parent Chain Job Application",
					"_Name": "Parent Chain Job Check",
					"_ConditionType": "RejectWhen",
					"_ErrorAttribute": "APPL_ID"
				},
				{
					"Condition": " (  Inserting  OR  Updating )   AND  (  ITM_TYP   <> 5 /* Folder */ AND Q_ID IS NULL /* Queue ID is empty */ ) ",
					"ErrorMessage": "Job Queue Id is required",
					"_Name": "QueueIDNotNullForJobs",
					"_ConditionType": "RejectWhen",
					"_ErrorAttribute": "Q_ID"
				}
			]
		},
